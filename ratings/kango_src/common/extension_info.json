{
    "name": "Ratings for FS.TO (ex FS.UA) and EX.UA",
    "description": "Shows films and series rating from IMDB and Kinopoisk on FS.TO (ex FS.UA) and EX.UA. Allows to vote in both systems in place.",
    "creator": "Ihor Kostiuk",
    "content_scripts": [
        "ts/common.js",
        "ts/xhr.js",
        "ts/Settings/Settings.js",
        "ts/Settings/KangoStorageSettings.js",
        "ts/InformationProviders/FSUAInformationProvider.js",
        "ts/InformationProviders/FSUAInformationProviderOld.js",
        "ts/InformationProviders/EXUAInformationProvider.js",
        "ts/Databases/imdb/ImdbInfo.js",
        "ts/Databases/imdb/ImdbLookuper.js",
        "ts/Databases/imdb/ImdbDatabaseInfo.js",
        "ts/Databases/kinopoisk/KpInfo.js",
        "ts/Databases/kinopoisk/KpLookuper.js",
        "ts/Databases/kinopoisk/KpDatabaseInfo_Helper.js",
        "ts/Databases/kinopoisk/KpDatabaseInfo.js",
        "js/md5.js",
        "js/tVote.js",
        "ts/Ratings.js",
        "ts/Init.js"
    ],
    "homepage_url": "https://github.com/ki0ki0/ratings_ext",
    "version": "2.2.1",
    "options_page": "options.html",
    "default_locale": "en",
    "permissions": {
        "tabs": false,
        "context_menu": false,
        "web_navigation": false,
        "storage": true,
        "content_scripts": [
            "http://fs.to/*",
            "http://brb.to/*",
            "http://ewq.to/*",
            "http://sdf.to/*",
            "http://cxz.to/",
            "http://www.ex.ua/*",
            "http://www.kinopoisk.ru/film/*"
        ],
        "xhr": [
            "http://www.imdb.com/*",
            "http://www.kinopoisk.ru/*",
            "http://ext.kinopoisk.ru/*",
            "https://ext.kinopoisk.ru/*"
        ]
    }
}